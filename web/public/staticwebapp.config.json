{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "*.{png,jpg,jpeg,gif,svg}",
      "*.{css,js}"
    ]
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html",
      "statusCode": 200
    }
  },
  "globalHeaders": {
    "X-Frame-Options": "DENY",
    "X-Content-Type-Options": "nosniff"
  },
  "platform": {
    "apiRuntime": "node:20"
  }
}
